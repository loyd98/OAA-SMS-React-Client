{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "prefer-destructuring": ["error", {
            "array": false,
            "object": false
          }, {
            "enforceForRenamedProperties": false
          }]
    }
}
